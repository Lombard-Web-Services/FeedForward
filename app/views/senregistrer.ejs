<body class="bodylogin">
  <div class="container-fluid senregistrermobile">
    <div class="row justify-content-center">
      <div class="pd col-11 col-sm-10 col-md-10 col-lg-6 col-xl-5 text-center p-0 mt-3 mb-2">
        <div class="card">
          <h2 id="heading" class="lang-titreenregistrezvous">Enregistrez vous</h2>
          <p class="lang-quizzysubtitle quizzy-subtitle">Pour utiliser les Quiz</p>
          <form id="inscription" class="inscription" role="form" method="post" action="#">
            <!-- progressbar -->
            <ul id="progressbar">
              <li class="active startariane" id="account"><strong class="lang-account">Créer un compte</strong></li>
              <li id="personal"><strong class="lang-personal">Informations</strong></li>
              <li id="Status"><strong class="lang-Status">Status</strong></li>
              <li id="confirm" class="endariane"><strong class="lang-confirm">Finalisation</strong></li>
            </ul>
            <div class="progress">
              <div class="progress-bar progress-bar-striped progress-bar-animated" role="progressbar" aria-valuemin="0" aria-valuemax="100"></div>
            </div> <br> <!-- fieldsets -->
            <fieldset>
              <div class="form-card">
                <div class="rowtitlestep">
                  <div class="col-7">
                    <h2 class="lang-compteutilisateur fs-title">Compte utilisateur:</h2>
                  </div>
                  <div class="col-5">
                    <h2 class="lang-steps1sur4 steps">Etape 1 / 4</h2>
                  </div>
                </div>
                <div class="form-wrapper">
                  <label class="lang-email fieldlabels">Email: *</label>
                  <input type="email" name="email" class="lang-emailplaceholder form-control" placeholder="Adresse de courriel" required="" />
                </div>
                <div class="form-wrapper">
                  <label class="lang-nomdutilisateur fieldlabels">Nom d'utilisateur: *</label>
                  <input type="text" name="uname" class="lang-nomdutilisateurplaceholder form-control" placeholder="Nom d'utilisateur" required="" />
                </div>
                <div class="form-wrapper">
                  <label class="lang-motdepasse fieldlabels">Mot de passe: *</label>
                  <input type="password" class="lang-motdepasseplaceholder form-control" name="pwd" placeholder="Mot de passe" id="password" required="" />
                </div>
                <div class="form-wrapper">
                  <label class="lang-confirmezvotremotdepasse fieldlabels">Confirmez votre mot de passe: *</label>
                  <input type="password" name="cpwd" class="lang-confirmezvotremotdepasseplaceholder form-control" placeholder="Confirmez votre mot de passe" required="" />
                </div>
              </div>
              <hr>
              <input type="button" name="next" class="lang-boutonsuivant next action-button next1" value="Suivant" />
            </fieldset>
            <fieldset>
              <div class="form-card">
                <div class="rowtitlestep">
                  <div class="col-7">
                    <h2 class="lang-titreinformations fs-title">Informations :</h2>
                  </div>
                  <div class="col-5">
                    <h2 class="lang-steps2sur4 steps">Etape 2 / 4</h2>
                  </div>
                </div> <label class="lang-prenomfield fieldlabels">Prénom: *</label> <input type="text" class="lang-prenomplaceholder form-control" name="fname" placeholder="Saisissez votre prénom" /> <label class="lang-nomfield fieldlabels">Nom: *</label> <input type="text" class="lang-nomfieldplaceholder form-control" name="lname" placeholder="Saisissez votre nom de famille" /> <label class="lang-numerodetelfield fieldlabels">Numéro de téléphone: *</label> <input type="text" name="phne" class="lang-numerodetelfieldplaceholder form-control" placeholder="Saississez votre numéro de téléphone" />
              </div>
              <hr>
              <input type="button" name="next" class="lang-boutonsuivant next action-button next2" value="Suivant" /> <input type="button" name="previous" class="lang-boutonprecedent previous action-button-previous left" value="Précédent" />
            </fieldset>
            <fieldset>
              <div class="form-card">
                <div class="rowtitlestep">
                  <div class="col-7">
                    <h2 class="lang-statusutilisateurtitre fs-title">Status utilisateur:</h2>
                  </div>
                  <div class="col-5">
                    <h2 class="lang-steps3sur4 steps">Etape 3 / 4</h2>
                  </div>
                </div>
                <div class="creauti">
                  <label class="switch">
                    <input type="checkbox" name="typeuser" id="usercreator" checked>
                    <span class="slider round"></span></label>
                  <span class="lang-utilisateurcreateur fieldlabels" style="margin-left:1em;line-height:2em;">Utilisateur / Créateur de Quiz</span>
                </div>
              </div>
              <hr>
              <input type="button" name="next" class="lang-boutonsuivant next action-button submit" value="Envoyer" style="bottom:0;" /> <input type="button" name="previous" class="lang-boutonprecedent previous action-button-previous left" value="Précédent" />
            </fieldset>
            <fieldset>
                <div class="form-card">
                  <div class="rowtitlestep">
                    <div class="col-7">
                      <h2 class="lang-finalstep fs-title">Etape finale:</h2>
                    </div>
                    <div class="col-5">
                      <h2 class="lang-steps4on4 steps">Etape 4 / 4</h2>
                    </div>
                  </div>
                  <div class="purple-text text-center"></div>
                  <div class="rowtitlestep">
                    <div class="success-animation">
                      <svg class="checkmark" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 52 52">
                        <circle class="checkmark__circle" cx="26" cy="26" r="25" fill="none" />
                        <path class="checkmark__check" fill="none" d="M14.1 27.2l7.1 7.2 16.7-16.8" />
                      </svg>
                      <div class="lang-success success" style="font-size:3em; text-align:center;color:#006DBF;">Enregistré</div>
                    </div>
                  </div>
                </div>
            </fieldset>
            <input type="hidden" name="fingerprint" value="" />
          </form>
        </div>
      </div>
    </div>
  </div>
